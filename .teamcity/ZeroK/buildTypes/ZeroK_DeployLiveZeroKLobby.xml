<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="b292fc67-4172-4a9e-a8a7-caed78c22294" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/9.0/project-config.xsd">
  <name>Deploy LIVE Zero-K Lobby</name>
  <description />
  <settings>
    <options>
      <option name="buildNumberPattern" value="%dep.ZeroK_StableLiveBuild.build.number%" />
    </options>
    <parameters />
    <build-runners>
      <runner id="RUNNER_22" name="Deploy ZKL" type="simpleRunner">
        <parameters>
          <param name="script.content"><![CDATA[copy /Y ZeroKLobby\bin\release\Zero-K.exe c:\projekty\zero-k.info\www\lobby
copy /Y ZeroKLobby\bin\release\Zero-K.version.txt c:\projekty\zero-k.info\www\lobby]]></param>
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
      <runner id="RUNNER_31" name="Deploy Chobby" type="simpleRunner">
        <parameters>
          <param name="script.content"><![CDATA[copy /Y ChobbyLauncher\bin\release\Chobby.exe c:\projekty\zero-k.info\www\lobby
copy /Y ChobbyLauncher\bin\release\Chobby.version.txt c:\projekty\zero-k.info\www\lobby]]></param>
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="ZeroK_ZeroKInfrastructure1_2" />
    </vcs-settings>
    <requirements />
    <build-triggers>
      <build-trigger id="vcsTrigger" type="vcsTrigger">
        <parameters>
          <param name="quietPeriodMode" value="DO_NOT_USE" />
          <param name="triggerRules"><![CDATA[+:ZeroKLobby/**
+:Shared/**
+:Libs/**
+:ChobbyLauncher/**]]></param>
        </parameters>
      </build-trigger>
    </build-triggers>
    <dependencies>
      <depend-on sourceBuildTypeId="ZeroK_StableLiveBuild">
        <options>
          <option name="run-build-if-dependency-failed" value="CANCEL" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

